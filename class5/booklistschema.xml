<!-- booklist
     bookname
     publishedYear
     author
        first
        last -->

<?xml version="2" encoding="utf=8">
<xs:Schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xml:element name="Booklist">
<xs:ComplexType>
<xs:sequence>
  <xs:element name="bookname" type="xs:String">
  <xs:element name="publishedyear" type="xs:String">
  <xs:element name="author">
  <xs:ComplexType>
    <xs:sequence>
        <xs:element name="firstname" type="xs:String">
        <xs:element name="lastname" type="xs:String">
    </xs:sequence>
  </xs:ComplexType>

</xs:sequence>
</xs:ComplexType>
</xs:Schema>